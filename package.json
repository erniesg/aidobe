{
  "name": "aidobe",
  "version": "1.0.0",
  "description": "AI-powered image and video generation platform with Cloudflare Workers",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy --env prod",
    "deploy:dev": "wrangler deploy --env dev",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:critical": "vitest run tests/unit/",
    "test:live": "./tests/e2e/live-api-test.sh",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "hono": "^4.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.2.0",
    "eslint": "^8.56.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "miniflare": "^4.20250617.4",
    "prettier": "^3.2.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0",
    "vitest-environment-miniflare": "^2.14.4",
    "wrangler": "^4.22.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erniesg/aidobe.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/erniesg/aidobe/issues"
  },
  "homepage": "https://github.com/erniesg/aidobe#readme"
}
